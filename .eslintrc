{
  "extends": "eslint:recommended",
  "parserOptions": {
    "ecmaVersion": 6
  },
  "env": {
    "amd": true,
    "browser": true,
    "worker": true,
    "es6": true
  },
  "rules": {
    "accessor-pairs": 2,
    "array-bracket-spacing": 0,
    "brace-style": 0,
    "camcelcase": 0,
    "comma-dangle": 0,
    "comma-spacing": [2, {"before": false, "after": true}],
    "comma-style": [2, "last"],
    "computed-property-spacing": [2, "never"],
    "consistent-return": 2,
    "curly": 2,
    "default-case": 2,
    "dot-location": [2, "property"],
    "dot-notation": 0,
    "eqeqeq": [2, "allow-null"],
    "generator-star-spacing": [2, "after"],
    "indent": 0,
    "init-declarations": 0,
    "new-parens": 2,
    "no-catch-shadow": 0,
    "no-cond-assign": [2, "except-parens"],
    "no-console": 0,
    "no-constant-condition": 2,
    "no-control-regex": 0,
    "no-dupe-args": 2,
    "no-dupe-keys": 2,
    "no-duplicate-case": 2,
    "no-empty": 2,
    "no-empty-character-class": 2,
    "no-fallthrough": 2,
    "no-invalid-this": 0,
    "no-nested-ternary": 2,
    "no-octal": 2,
    "no-redeclare": 2,
    "no-return-assign": [2, "always"],
    "no-sequences": 2,
    "no-shadow": 2,
    "no-spaced-func": 2,
    "no-undef": 2,
    "no-unexpected-multiline": 2,
    "no-unused-vars": [1, {"vars": "all", "args": "after-used"}],
    "no-use-before-define": [1, "nofunc"],
    "no-useless-call": 2,
    "no-void": 2,
    "no-with": 2,
    "padded-blocks": [2, "never"],
    "require-yield": 2,
    "semi": [2, "always"],
    "semi-spacing": 0,
    "space-in-parens": 0,
    "strict": [2, "function"],
    "quotes": [2, "single", "avoid-escape"],
    "valid-jsdoc": [0, {
      "prefer": {
        "return": "returns"
      },
      "requireReturn": false,
      "requireParamDescription": false
    }],
    "valid-typeof": 2
  }
}
